{
  "name": "medshare",
  "version": "1.0.0",
  "description": "Medshare project",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cd server/ && npm start",
    "server": "nodemon server/app.js",
